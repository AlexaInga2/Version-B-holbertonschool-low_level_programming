#include "holberton.h"
/**
 * _strncat - Makes the sum of two numbers
 * @dest: First operand
 * @src: Second operand
 * @n:
 *
 * Return: dest
 */
char *_strncat(char *dest, char *src, int n)
{
	int i, j;

	for (i = 0; dest[i]; i++)
		;
	for (j = 0; j < n; j++, i++)
		dest[i] = src[j];
	dest[i] = '\0';

	return (dest);
}
